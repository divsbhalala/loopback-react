{
  "name": "loopback-react",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "watch": "nohup nodemon ./server/server.js </dev/null & cd ./client; yarn start",
    "posttest": "npm run lint && nsp check",
    "kill": "kill $(lsof -t -i:3000,3003)"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^1.3.0",
    "loopback": "^3.26.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.4.0",
    "loopback-component-passport": "^3.11.0",
    "loopback-component-storage": "^3.6.3",
    "loopback-connector-mongodb": "^5.0.0",
    "nodemailer-mandrill-transport": "^1.2.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.23.0",
    "strong-error-handler": "^3.2.0",
    "unique-filename": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-loopback": "^13.1.0",
    "nodemon": "^1.19.1",
    "nsp": "^3.2.1"
  },
  "nodemonConfig": {
    "delay": 500,
    "watch": [
      "common",
      "server"
    ],
    "ext": "js,json",
    "exec": "yarn start"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "MIT",
  "description": "loopback-react"
}
